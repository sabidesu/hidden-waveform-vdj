<!-- 
		****************************  
				RACK STACK 
		****************************  
-->
<!-- 
			rack 1 => video
			rack 2 => effects
			rack 3 => master rec // no longer used
			rack 4 => mixer
-->
 
<panel name="racks" condition="true">

	<!-- full decks  -->
	<pos x="+10" y="1080-512-10+1" condition="var_equal '@$decklayout' 1 && var_equal '@$4decks' 0"/>
	<pos x="+10" y="1080-512-10+1+120" condition="var_equal '@$decklayout' 1 && var_not_equal '@$4decks' 0"/>

	<!-- compact decks  -->
	<pos x="+10" y="+43+173+220+1" condition="var_equal '@$decklayout' 0 && var_equal '@$4decks' 0"/>
	<pos x="+10" y="+43+173+220+1+220" condition="var_equal '@$decklayout' 0 && var_not_equal '@$4decks' 0"/>

	<!-- small decks  -->
	<pos x="+10" y="+43+173+120+1+6" condition="var_equal '@$decklayout' 2 && var_equal '@$4decks' 0"/>
	<pos x="+10" y="+43+173+120+1+120+9" condition="var_equal '@$decklayout' 2 && var_not_equal '@$4decks' 0"/>

	<!--  racks -->
	<panel class="mix_rack" visibility="var_equal '$mix_rack' 1"/>
	<panel class="video_rack" visibility="var_equal '$video_rack' 1"/>
	<panel class="fx_rack" visibility="var_equal '$fx_rack' 1"/>

	<!-- single racks -->
	<panel class="mix_rack" visibility="var_equal '$rack' 4"/>
	<panel class="video_rack" visibility="var_equal '$rack' 1"/>
	<panel class="fx_rack" visibility="var_equal '$rack' 2"/>

</panel>


</panel><!-- end browser not zoomed -->

