<define class="show_browser" placeholders="*height,*down">
	<pos x="+10" y="+1080-[DOWN]-10"/>

	<!-- multi rack view -->	
	<group x="+0" y="+0">
		<browser class="browser" condition="var_equal '$singlerack' 0">

			<!-- 4 rack -->
			<pos x="+0" y="+80+80+80+120" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80-80-120" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>

			<!-- 3 rack -->

			<!-- fx +X -->
			<pos x="+0" y="+80+80+120" condition="var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80-120" condition="var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>

			<!-- mix +X -->
			<pos x="+0" y="+80+80+120" condition="var_equal '$mix_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80-120" condition="var_equal '$mix_rack' 1 && var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>

			<pos x="+0" y="+80+80+120" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80-120" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$video_rack' 1"/>

			<pos x="+0" y="+80+80+80" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80-80" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1"/>

			<!-- 2 rack -->

			<!-- rec +X -->
			<pos x="+0" y="+80+120" condition="var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-120" condition="var_equal '$rec_rack' 1 && var_equal '$video_rack' 1"/>

			<!-- fx +X -->
			<pos x="+0" y="+80+120" condition="var_equal '$fx_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-120" condition="var_equal '$fx_rack' 1 && var_equal '$video_rack' 1"/>

			<pos x="+0" y="+80+80" condition="var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80" condition="var_equal '$fx_rack' 1 && var_equal '$rec_rack' 1"/>

			<!-- mix +X -->
			<pos x="+0" y="+80+120" condition="var_equal '$mix_rack' 1 && var_equal '$video_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-120" condition="var_equal '$mix_rack' 1 && var_equal '$video_rack' 1"/>

			<pos x="+0" y="+80+80" condition="var_equal '$mix_rack' 1 && var_equal '$rec_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80" condition="var_equal '$mix_rack' 1 && var_equal '$rec_rack' 1"/>

			<pos x="+0" y="+80+80" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80-80" condition="var_equal '$mix_rack' 1 && var_equal '$fx_rack' 1"/>


			<!-- 1 rack -->
			<pos x="+0" y="+120" condition="var_equal '$6deck_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-120" condition="var_equal '$6deck_rack' 1"/>
			<pos x="+0" y="+120" condition="var_equal '$video_rack' 1"/>
			<size width="1920-22" height="[HEIGHT]-120" condition="var_equal '$video_rack' 1"/>
			<pos x="+0" y="+80" condition="var_equal '$fx_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$fx_rack' 1"/>
			<pos x="+0" y="+80" condition="var_equal '$rec_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$rec_rack' 1"/>
			<pos x="+0" y="+80" condition="var_equal '$mix_rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$mix_rack' 1"/>

			<!-- default-->
			<pos x="+0" y="+0"/>
			<size width="1920-20" height="[HEIGHT]"/>
			<zoomed x="10" y="43+120+5" width="1920-20" height="1080-43-120-5-10"/>
		</browser>

		<!-- single rack view -->
		<browser class="browser" condition="var_equal '$singlerack' 1" border="red" border_size="1">
			<!-- 1 rack -->
			<pos x="+0" y="+80" condition="var_equal '$rack' 4"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$rack' 4"/>
			<pos x="+0" y="+120" condition="var_equal '$rack' 1"/>
			<size width="1920-20" height="[HEIGHT]-120" condition="var_equal '$rack' 1"/>
			<pos x="+0" y="+80" condition="var_equal '$rack' 2"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$rack' 2"/>
			<pos x="+0" y="+80" condition="var_equal '$rack' 3"/>
			<size width="1920-20" height="[HEIGHT]-80" condition="var_equal '$rack' 3"/>
			<!-- default-->
			<pos x="+0" y="+0"/>
			<size width="1920-20" height="[HEIGHT]"/>
			<zoomed x="10" y="43+120+5" width="1920-20" height="1080-43-120-5-10"/>
		</browser>		
		
	</group>

</define>


	<!-- full decks -->
	<panel class="show_browser" down="512" height="512" condition="var_equal '@$decklayout' 1 && var_equal '@$4decks' 0"/>
