****************************  
-->   

<define class="mix_rack">
	<panel name="mix_rack" visible="yes">

		<pos x="+0" y="+0"/><!-- top rack slot -->

		<panel class="rack_background" background="background" height="80"/> 
		<panel class="rack_tab" text="MASTER" localize="true" height="82"/> 
		<panel class="rack_tab" x="+1920-20-24" text="MIC" localize="true" orientation="vertical-cw" height="82"/> 



		<visual name="center_mixer">
			<pos x="+960-150-10" y="+0"/>
			<size width="300" height="82"/>
			<off color="mixerbackground" shape="square" border="bordercolor" border_size="1"/>
		</visual>


		<group name="master_output">
			<deck deck="master">
			<group name="master_vol" x="+80" y="+10">
				<group name="ma_vol" x="+0" y="+0" >
					<panel class="knob_modern" x="+0" y="+10">
						<slider action="master_volume" dblclick="master_volume 100%" rightclick="master_volume 100% ? master_volume 20% : master_volume 100%"/>
						<textzone><text localize="true" text="MASTER"/></textzone>
					</panel>
				</group>
			</group>
			<group name="hp_vol" x="+170" y="+10">
				<line x="+0" y="+10" width="1" height="45" color="dark" hightlight="hightlight" shadow="shadow"/>
				<group name="hp_vol" x="+40" y="+0" >
					<panel class="knob_modern" x="+0" y="+10">
						<slider action="headphone_volume" dblclick="headphone_volume 100%" rightclick="headphone_volume 100% ? headphone_volume 0% : headphone_volume 100%" disabled="not get_hasheadphones"/>
						<textzone><text localize="true" text="VOL"/></textzone>
