	<visual>
		<pos x="+0" y="+1"/>
		<size width="[WIDTH]" height="[HEIGHT]-1"/>
		<off color="[BACKGROUND]" shape="square"/>
	</visual>
	<line name="rack_top" x="+0" y="+0" width="[WIDTH]" height="1" color="bordercolor" />
	<line name="rack_bottom" x="+0" y="+[HEIGHT]" width="[WIDTH]" height="1" color="bordercolor" />
</define>

<define class="rack_tab" placeholders="pos=+0,background=decktabs,height=80,orientation=vertical,text">
	<button>
		<pos x="+0" y="+0"/>
		<size width="24" height="[HEIGHT]"/>
		<off color="[BACKGROUND]" shape="square" border="bordercolor" border_size="1"/>
		<text fontsize="10" color="textdark" orientation="[ORIENTATION]" align="center" text="[TEXT]" localize="true"/>
	</button>
</define>

<define class="rack_tab4" placeholders="action,pos=+0,background=decktabs,height=80,orientation=vertical,text">
	<button action="[ACTION]" query="select">
		<pos x="+0" y="+0"/>
		<size width="24" height="[HEIGHT]"/>
		<off color="[BACKGROUND]" shape="square" border="deckcolor" border_size="1"/>
		<on color="[BACKGROUND]" shape="square" border="deckcolor" border_size="1"/>
		<text fontsize="10" color="deckcolor" orientation="[ORIENTATION]" align="center" text="[TEXT]" localize="true"/>
	</button>
</define>
